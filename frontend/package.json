	"scripts": {
		"precommit": "lint-staged",
		"e2e-testing:merge-packages": "package-json-merge package.json ./cypress/package.json > package_temp.json",
		"start": "react-scripts start",
		"build": "node node_modules/react-scripts/bin/react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"analyze": "source-map-explorer build/static/js/main.*",
		"start:json-server": "json-server --watch  json-mock-server/db.js -p 3001 -m  json-mock-server/middleware.js -r  json-mock-server/routes.json",
		"prettify": "./node_modules/.bin/prettier --write src/**/*.js",
		"prettify:file": "./node_modules/.bin/prettier --write",
		"lint": "eslint \"src/**/*.{js,jsx,json,css}\"",
		"mocks": "cross-env REACT_APP_WITH_JSON_SERVER=true concurrently  \"npm run start:json-server\" \"npm run start\" \"node webSockets-mock/server.js\""
	},
	"prettier": {
		"printWidth":
		 100,
		"useTabs": true,
		"tabWidth": 2,
		"semi": true,
		"singleQuote": true,
		"jsxSingleQuote": true,
		"trailingComma": "none",
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"requirePragma": false,
		"endOfLine": "auto"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,json,css}": [
			"./node_modules/.bin/prettier --write",
			"git add"
		]
	},
	"proxy": "http://localhost:3001"
}
